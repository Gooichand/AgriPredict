# KisanSafe Environment Variables
# Copy this file to .env.local and fill in your actual values

# AI Service API Keys (at least one required)
OPENAI_API_KEY=your_openai_key_here
GEMINI_API_KEY=your_gemini_key_here
HUGGING_FACE_API_KEY=your_hf_token_here

# Client-side API Keys (for direct browser calls)
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_key_here

# Weather API
NEXT_PUBLIC_OPENWEATHER_API_KEY=your_openweather_key_here

# Database (if using)
DATABASE_URL=your_database_url_here

# Deployment
NEXT_PUBLIC_APP_URL=https://your-domain.com